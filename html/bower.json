{
  "name": "nw-frontend-kit",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "modernizr": "~2.8.1",
    "font-awesome": "fontawesome#~4.4.0",
    "jquery-slimscroll": "slimScroll#^1.3.8",
    "fancybox": "^2.1.5",
    "video.js": "^5.12.6",
    "owl.carousel": "^2.2.0"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "fancybox": {
      "main": [
        "source/*.css",
        "source/jquery.fancybox.pack.js",
        "lib/jquery.mousewheel-3.0.6.pack.js"
      ]
    },
    "jquery-slimscroll": {
      "main": [
        "*.css",
        "jquery.slimscroll.min.js"
      ]
    },
    "font-awesome": {
      "main": [
        "scss/font-awesome.scss",
        "fonts/*"
      ]
    }
  },
  "devDependencies": {
    "chai": "~3.4.2",
    "mocha": "~2.3.4"
  }
}
